<h1>Artists</h1>
<ul>
  <% @artists.each do |artist| %>
    <li><%= link_to artist.name, artist %></li>
    <%= link_to "View", artist %>
    <%= link_to "Edit", edit_artist_path(artist) %>
    <%= link_to "Delete", artist_path(artist), method: :delete %>
  <% end %>
</ul>
<hr/>
<%= link_to 'Create a new artist', new_artist_path %>
<%= link_to 'Billboards', billboards_path %>